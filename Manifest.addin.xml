<?xml version="1.0" encoding="UTF-8"?>
<Addin namespace="MonoDevelop" id="LarrysFSharpTemplates.iOS" name="Larry's F# Templates" author="Larry OBrien" copyright="(c) 2014-2016 Larry OBrien" description="F# Templates for Exploring Xamarin.iOS" category="Project Types" version="2.0.7">
	<Runtime>
		<!---iOS files -->
		<Import file="templates/F#/SingleFileiOS.xpt.xml"/>
		<Import file="templates/C#/SingleFileiOS.xpt.xml"/>
		
	</Runtime>
	<!--- Common dependencies -->
	<Dependencies>
		<Addin id="Core" version="6.0"/>
		<Addin id="Ide" version="6.0"/>
	</Dependencies>
	<Module>
		<Extension path="/MonoDevelop/Ide/ProjectTemplates">
			<ProjectTemplate id="SingleFileFSharpiOS" file="templates/F#/SingleFileiOS.xpt.xml"/>
		</Extension>
		<Extension path="/MonoDevelop/Ide/ProjectTemplates">
			<ProjectTemplate id="SingleFileCSharpiOS" file="templates/C#/SingleFileiOS.xpt.xml"/>
		</Extension>
	</Module>
</Addin>
